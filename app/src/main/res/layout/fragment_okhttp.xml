<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:orientation="vertical">

    <TextView
        android:id="@+id/tv_url_conn_get"
        style="@style/EntryItem"
        android:text="HttpUrlConnection GET"/>
    
    <TextView
        android:id="@+id/tv_okhttp_get"
        style="@style/EntryItem"
        android:text="OkHttp GET"/>

    <TextView
        android:id="@+id/tv_okhttp_post"
        style="@style/EntryItem"
        android:text="OkHttp POST"/>

    <TextView
        android:id="@+id/tv_cache"
        style="@style/EntryItem"
        android:text="Cache"/>

    <TextView
        android:id="@+id/tv_simple_okhttp"
        style="@style/EntryItem"
        android:text="SimpleOkHttp"/>

    <TextView
        android:id="@+id/tv_download_image"
        style="@style/EntryItem"
        android:text="下载图片"/>
    
    <TextView
        android:id="@+id/tv_upload_file"
        style="@style/EntryItem"
        android:text="上传文件"/>

</LinearLayout>